<FindbugsPlugin xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xsi:noNamespaceSchemaLocation="findbugsplugin.xsd"
          pluginid="edu.umd.cs.findbugs.plugins.jdbcCloud" defaultenabled="true"
          provider="FindBugs project" website="http://findbugs.sourceforge.net">

          <Cloud id="edu.umd.cs.findbugs.cloud.db.Default"
                    cloudClass="edu.umd.cs.findbugs.cloud.db.DBCloud"
                    usernameClass="edu.umd.cs.findbugs.cloud.username.AppEngineNameLookup"/>
          <Cloud id="edu.umd.cs.findbugs.cloud.db.DB-UserName"
                    cloudClass="edu.umd.cs.findbugs.cloud.db.DBCloud"
                    usernameClass="edu.umd.cs.findbugs.cloud.username.LocalNameLookup" hidden="true"/>
</FindbugsPlugin>
