

<!--
  Plugin descriptor for FindBugs "core" plugin.
  This plugin is enabled by default.
-->
<FindbugsPlugin xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
   				xsi:noNamespaceSchemaLocation="findbugsplugin.xsd"
				pluginid="edu.umd.cs.findbugs.plugins.appengine"
                defaultenabled="true"
                provider="FindBugs project" 
                website="http://findbugs.sourceforge.net"
                >

  <Cloud id="edu.umd.cs.findbugs.cloud.appengine.theflybush"				
	 cloudClass="edu.umd.cs.findbugs.cloud.appEngine.WebCloudClient"
	 usernameClass="edu.umd.cs.findbugs.cloud.username.AppEngineNameLookup"
         disabled="true" onlineStorage="true"
	>
       <Property key="appengine.host">http://theflybush.appspot.com</Property>
</Cloud>
  <Cloud id="edu.umd.cs.findbugs.cloud.appengine.findbugs-cloud"				
	 cloudClass="edu.umd.cs.findbugs.cloud.appEngine.WebCloudClient"
	 usernameClass="edu.umd.cs.findbugs.cloud.username.AppEngineNameLookup" onlineStorage="true"
	>
       <Property key="appengine.host">http://findbugs-cloud.appspot.com</Property>
</Cloud>

</FindbugsPlugin>
