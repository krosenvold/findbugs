
<project name="AppEngineCloudClient" default="plugin">

<property name="classes.dir" value="build/classes"/>
        

 <target name="copyXML" >
 <copy todir="${classes.dir}">
	<fileset dir="etc">
		<include name="*.xml"/>
	</fileset>
	</copy>
</target>

 <target name="plugin" depends="copyXML" >
        <jar destfile="plugin.jar">
        <fileset dir="etc">
		<include name="*.xml"/>
	</fileset>
        <fileset dir="${classes.dir}">
			<include name="**/*.class"/>
		</fileset>
        <fileset dir="../AppEngineCloudProtocol/build">
			<include name="**/*.class"/>
		</fileset>
	 <zipfileset src="../AppEngineCloudProtocol/lib/protobuf-java-2.2.0.jar"/>
	</jar>
</target>
</project>

      
